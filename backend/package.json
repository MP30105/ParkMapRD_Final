{
  "name": "parkmaprd-backend",
  "version": "1.0.1",
  "main": "server.js",
  "license": "MIT",
  "type": "commonjs",
  "scripts": {
    "start": "node server.js",
    "restart:win": "powershell -ExecutionPolicy Bypass -File scripts/restart-backend.ps1",
    "integration": "node demo_run.js",
    "test": "jest --runInBand --config tests/jest.config.js",
    "test:unit": "jest tests/unit --runInBand --config tests/jest.config.js",
    "test:integration": "jest tests/integration --runInBand --config tests/jest.config.js",
    "test:performance": "jest tests/performance --runInBand --config tests/jest.config.js --testTimeout=30000",
    "test:watch": "jest --watch --runInBand --config tests/jest.config.js",
    "test:coverage": "jest --coverage --runInBand --config tests/jest.config.js",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:performance"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "jest": "^29.6.1",
    "supertest": "^6.3.3"
  },
  "description": "PARKMAPRD backend API",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.21.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.7",
    "qrcode": "^1.5.3",
    "sql.js": "^1.10.2",
    "stripe": "^14.9.0",
    "uuid": "^9.0.1",
    "ws": "^8.18.3",
    "zod": "^3.25.76"
  }
}
